    "@types/webpack": "^3.8.5",
    "jest": "^22.2.1",
    "memory-fs": "^0.4.1",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.0.3",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.6.0",
    "tslint-config-prettier": "^1.7.0",
    "tslint-language-service": "^0.9.8",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.7.1",
    "typescript-snapshots-plugin": "^1.1.0",
